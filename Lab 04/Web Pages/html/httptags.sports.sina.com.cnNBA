			if(data.desc===''){
				contClz += ' no-txt';
			}
			html.push('<div class="img fl">');
			if(noPic){
				contClz +=' no-img';
			}else{
				html.push('<img src="http://s.img.mix.sina.com.cn/auto/resize?img='+encodeURIComponent(pic)+'&size=136_170" height="170" width="136" alt="'+tag+'">');
			}
			html.push('</div>');
			html.push('<div class="txt">');
			html.push('<div class="hd clearfix">');
			html.push('<span class="name fl"><i></i><em>'+tag+'</em></span>');
			if(allData.tag.status === 0){
				html.push('<a class="btn-a fl" href="javascript:;" title="订阅"><i></i>订阅</a>');
			}else{
				html.push('<a class="btn-a btn-a-done" href="javascript:;" title="已订阅"><i></i>已订阅</a>');
			}
			html.push('</div>');
			html.push('<div class="bd">'+ strSplit(data.desc,270)+'&nbsp;');
			if(data.desc){
				html.push('<a href="'+data.url+'" target="_blank">详情&gt;&gt;</a>');
			}
			html.push('</div>');
			html.push('</div>');
			html = html.join('');
			dom.cont.addClass(contClz);
			dom.cont.html(html).show();
		};
		var renderTag = function(item){
			var clz = 'btn-b';
			if(item.status===1){
				clz += ' btn-b-done';
			}
			item.url = item.url ||('http://'+location.host+'/'+item.name);
			return '<span class="'+clz+'" href="javascript:;" tag="'+item.name+'"><a href="'+item.url+'" class="t">'+item.name+'</a><a href="javascript:;" class="i"></a></span>';
		};
		var renderTagList = function(listDom,listData){
			if(typeof listData === 'undefined'|| ($.isArray(listData)&&listData.length===0)){
				return;
			}
			var html = [];
			for (var i = 0, len = listData.length; i < len; i++) {
				var item = listData[i];
				html.push(renderTag(item));
			}
			listDom.html(html.join(''));
			listDom.parent().show();
		};
		renderCont();
		renderTagList(dom.tagList,allData.tagList);
		renderTagList(dom.myTagList,allData.myTagList);
	};
	var strSplit = (function(){
        var byteLen = function(str){
            if(typeof str == "undefined"){
                return 0;
            }
            var aMatch = str.match(/[^\x00-\x80]/g);
            return (str.length + (!aMatch ? 0 : aMatch.length));
        };
        return function(str,len){
            var s = str.replace(/\*/g, " ").replace(/[^\x00-\xff]/g, "**");
            str = str.slice(0, s.slice(0, len).replace(/\*\*/g, " ").replace(/\*/g, "").length);
            if(byteLen(str) > len) str = str.slice(0,str.length -1);
            return str;
        };
    })();
	var getNodes = function(wrap) {
        var attr = 'node-type';
        var wrap = $('#' + wrap);
        var nodes = $("[" + attr + "]", wrap);
        var nodesObj = {};
        nodesObj.wrap = wrap;
        nodes.each(function(i) {
            var item = $(this);
            nodesObj[item.attr(attr)] = item;
        });
        return nodesObj;
    };
    var bind = function(dom){
    	var over = function(item){
    		var hoverClz = 'btn-b-hover';
    		if(item.hasClass('btn-b-done')){
    			hoverClz = hoverClz+' btn-b-done-hover';
    		}
    		item.addClass(hoverClz);
    	};
    	var out = function(item){
    		var hoverClz = 'btn-b-hover';
    		if(item.hasClass('btn-b-done')){
    			hoverClz = hoverClz+' btn-b-done-hover';
    		}
    		item.removeClass(hoverClz);
    	};
    	dom.tagList.on('mouseover','.btn-b',function(){
    		over($(this));
    	});
    	dom.tagList.on('mouseout','.btn-b',function(){
    		out($(this));
    	});
    	// 已订阅标签
    	dom.myTagList.on('mouseover','.btn-b',function(){
    		over($(this));
    	});
    	dom.myTagList.on('mouseout','.btn-b',function(){
    		out($(this));
    	});
    	// 订阅
    	dom.tagList.on('click','.i',function(){
    		var item = $(this);
    		subscribe(item,PAGECFG,'b');
    	});
    	dom.myTagList.on('click','.i',function(){
    		var item = $(this);
    		subscribe(item,PAGECFG,'b');
    	});
    	dom.cont.on('click','.btn-a',function(){
    		var item = $(this);
    		subscribe(item,PAGECFG,'a');
    	});
    };
    var dom = {
    	cont:$('#SI_Cont'),
    	tagList:$('#SI_Tags'),
    	myTagList:$('#SI_MyTagList')
    };

	render(dom,PAGECFG);
    bind(dom);
})(window.__SinaTopBar__);
</script>
