<script type="text/javascript">
(function(Topbar){

	var needReload = false;


    // 登录
    Topbar.user.init(document.getElementById('SI_User'),{
        // entry 默认为 account，各频道不一样，可向通行证@龚翼 申请
        entry:'account',
        // 登录成功
        login_success:function(){
        	if(needReload){
	            location.href = location.href;
        	}
        },
        // 退出成功
        logout_success:function(){
            location.href = location.href;
        }
    });
    // 提示
    var Tip = (function(){
    	var wrap = null;
    	var calculator = null;
    	var timer = null;
    	return {
    		show:function(ele,html,time){
    			clearTimeout(timer);
    			var self = this;
    			if(!wrap){
    				wrap = $('<div class="subscribe-tip" style="display:none;"></div>').appendTo(document.body);
    			}
    			wrap.hide();
    			var offset = ele.offset();
    			wrap.html(html);
    			wrap.show().css({top:0, left:0});
    			wrap.css({
    			  top: (offset.top + wrap.outerHeight()) + 'px',
    			  left: (offset.left - wrap.outerWidth()/2) + 'px'
    			});
    			if(time){
    				timer = setTimeout(function(){
    					self.hide();
    				},time);
    			}
    		},
    		hide:function(){
    			wrap.hide();
    		}
    	};
    })();
	var timeoutHandle = (function(){
	    var events = [];
	    var handle = {
	        success:function(id){
	            var eve = events[id];
	            if(!eve){
	                return;
	            }
	            eve.isSuccess = true;
	            clearTimeout(eve.timer);

	        },
	        timeout:function(id,fn){
	            var eve = events[id];
	            if(!eve){
	                return;
	            }
	            eve.timer = setTimeout(function(){
	                if(eve.isSuccess){
	                    return;
	                }
	                if(typeof fn == 'function'){
	                    fn.call(this);
	                }
	            },eve.time);
	        }
	    };
	    return function(id,fn,time){
	        if(events[id]){
	            throw new Error(id+'已经被占用');
	            return;
	        }
	        events[id] = {};
	        events[id].time = time||5e3;
	        events[id].isSuccess = false;
	        if(typeof fn == 'function'){
	            fn.call(this,handle);
	        }
	    }
	})();
	var ijax = function(url,data,cb){
		var body = $('body');
		var suffix = Math.abs((new Date()).getTime()) + '_' + Math.round(Math.random() * 1e8);
		var id = "ijax_iframe_"+suffix;
		var ifm = $('<iframe frameborder="0" name="'+id+'" id="'+id+'"></iframe>');
		var inputHtml = (function(data){
			var html = [];
			for(var name in data){
				var val = data[name];
				html.push('<input type="hidden" name="'+name+'" value="'+val+'" />');
			}
			return html.join('');
		})(data);
		window['ijax_'+suffix] = function(msg){
			if(typeof cb === 'function'){
				cb(msg);
			}
			setTimeout(function(){
				ifm.remove();
				form.remove();
			},1e3);
		};
		var form = $('<form action="'+url+'?callback=ijax_'+suffix+'" method="post" target="'+id+'">'+inputHtml+'</form>');
		body.append(ifm);
		body.append(form);
		form.submit();

	};
	var subscribe = (function(){
		var adding = false;

		var add = function(allData,word,cb){
			var data = {
				project_id:allData.projectId,
				// data:'[{c:"'+ word +'",w:0}]',
				'data[0][c]':word,
				'data[0][w]':0,
				token:'',
				dt:'',
				from:'pc'
			};
			ijax('http://user.mix.sina.com.cn/subscribe/api/subscribe/update',data,function(msg){
				cb(msg);
			});
		};
		var remove = function(allData,word,cb){
			var data = {
				project_id:allData.projectId,
				'data[0][c]':word,
				token:'',
				dt:'',
				from:'pc'
			};
			ijax('http://user.mix.sina.com.cn/subscribe/api/subscribe/del',data,function(msg){
				cb(msg);
			});
		};
		var changeBtn = function(wrap,btnType){
			if(btnType==='a'){
				if (wrap.hasClass('btn-a-done')) {
					wrap.html('<i></i>订阅');
					wrap.removeClass('btn-a-done');
				} else {
					wrap.html('已订阅');
					wrap.addClass('btn-a-done');
				}
			}else{
				var type = 'add';
				if (wrap.hasClass('btn-b-done')) {
					type = 'remove';
					wrap.removeClass('btn-b-done btn-b-done-hover');
				} else {
					wrap.addClass('btn-b-done btn-b-done-hover');
				}
				toggleBtn(wrap,type);
			}

		};
		var toggleBtn = function(wrap,type){
			var isHot = wrap.parents('[node-type=tagList]').length>0;
			if(isHot){
				var oldWrap = dom.myTagList.find('[tag='+wrap.text()+']');
				if(type==='add'){
					// add时，如果是在hot中，复制一份并添加到my中
					if(oldWrap.length===0){
						var newWrap = wrap.clone();
						newWrap.appendTo(dom.myTagList);
						newWrap.removeClass('btn-b-hover btn-b-done-hover');
					}
				}else{
					oldWrap.remove();
				}

				// remove时，在my中找到相同的标签删掉
			}else{
				// 在my中直接删掉
				if(type==='remove'){
					wrap.remove();
				}
			}
			dom.myTagList.parent().show();

		};
		return function(item,allData,btnType){
			// 判断是否登录
			var UserPanel = window.SINA_USER_PANEL;
			LoginLayer = UserPanel.getOutLogin();
			if(!LoginLayer.isLogin()){
				try{
					UserPanel.setOutLoginMiddle(true);
					UserPanel.getOutLogin().show();
					return;
				}catch(e){

				}
				needReload = true;
			}
			var wrap = item.parent();
			var word = wrap.find('.t').text();
			if(btnType === 'a'){
				wrap = item;
				word = PAGECFG.tag.name;
			}
			if(adding){
				Tip.show(item,'您操作太快了，请休息一会！',2e3);
				return;
			}

			// 订阅类型 add为添加 remove为删除
			var type = wrap.hasClass('btn-b-done')?'remove':'add';
			if(btnType === 'a'){
				type = wrap.hasClass('btn-a-done')?'remove':'add';
			}
			adding = true;
			var TIMEOUT_NAME = 'TD_'+ (new Date()).getTime();
			timeoutHandle(TIMEOUT_NAME,function(handle){
			    handle.timeout(TIMEOUT_NAME,function(){
			        adding = false;
			    });
			    if(type==='add'){
			    	add(allData, word,function(msg){
			    		if(msg&&msg.result&&msg.result.status.code===0){
				    		changeBtn(wrap,btnType);
			    		}else{
			    			Tip.show(item,'服务器罢工，请稍后再试！',2e3);
			    		}
			    		adding = false;
			    		// 成功通知
			    		handle.success(TIMEOUT_NAME);

			    	});
			    }else{
			    	remove(allData, word,function(msg){
			    		if(msg&&msg.result&&msg.result.status.code===0){
				    		changeBtn(wrap,btnType);
			    		}else{
			    			Tip.show(item,'服务器罢工，请稍后再试！',2e3);
			    		}
			    		adding = false;
			    		// 成功通知
			    		handle.success(TIMEOUT_NAME);

			    	});
			    }

			},2e3);

		};
	})();
	var render = function(dom,allData){
		var renderCont = function(){
			var contClz = '';
			var tag = PAGECFG.tag.name;
			var kuEnt = allData.kuEnt;
			// 渲染图片和内容
			var data = {};
			var html = [];
			var pic = 'http://www.sinaimg.cn/dy/deco/2014/1215/d.png';
			var noPic = true;
