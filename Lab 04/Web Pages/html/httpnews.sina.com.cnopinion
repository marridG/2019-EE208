<script type="text/javascript">
    requirejs(['fixed','underscore','$'],function(Fixed){
        $(document).ready(function(){
            var cls='fixed_';
            var nav=$("#nav");
            var fixed;
            var initPos=nav.position().top;
            var closed=false;
            function open(){
                if(!(fixed instanceof Fixed)){
                    fixed=new Fixed({
                        id: 'nav',
                        top: 0
                    });
                }else{
                    fixed.init();
                }
                nav.addClass(cls);
            }
            function close(){
                if(fixed instanceof Fixed){
                    fixed.destory();
                    nav.removeClass(cls);
                }
            }
            function check(){
                var scrollTop=$(window).scrollTop();
                if(!closed){
                    if(scrollTop>=initPos){
                        open();
                    }else{
                        close();
                    }
                }
            }
            nav.on('click','.close',function(){
                closed=true;
                close();
            });
            var checkThrottle= _.throttle(check, 650);
            $(window).on('load scroll resize', checkThrottle);
        });
    });
</script>
