                                    loginLayer.set('sso', {
                                        entry : 'finance'
                                    }).init();

                                    panelSTK.addEvent(panelSTK.E("top-nr-login"), 'click', function(){
                                        loginLayer.set('plugin',{
                                            parentNode: document.body,
                                            position: 'custom'
                                        });
                                        loginLayer.set('styles',{
                                            'top': loginBtn.offset().top+25 + 'px',
                                            'left': loginBtn.offset().left-350 + 'px',
                                            'z-index': '99999'
                                        });
                                        loginLayer.show();
                                    });
                                }
                            },
                            triggerEvent: 'hover',//触发显示登录浮层的事件，可选择hover或click
                            loginBtns: ["top-nr-login"]
//					setPos: true
                        }).set('container',{//设置用户面板的父节点，不设置则为body
