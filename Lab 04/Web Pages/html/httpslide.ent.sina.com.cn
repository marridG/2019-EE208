                    if(n==j){
                        s4 = s.slice(0, (n>>1)-1);
                        return s4+ELLIPSIS;
                    }else{
                        s4 = s.slice(0, n>>1);
                        return s4;
                    }
                default:
                    var k = n - i,
                        s3 = s.slice(k, n),
