var init_user_info = function(){
	/*获取用户信息 start*/
	var sinaUser = sinaSSOManager.getSinaCookie();

	if(sinaUser){
		var weibo_account = sinaUser.uid;

		var url = 'http://mblogv2.city.sina.com.cn/interface/tcommonv2/no_auth/user/json_get_user_info_by_uid.php?t=jsonp&uid='+weibo_account+'&app_id=197&has_extend=0&callback=?';

		$.getJSON(url, function(json){
			if(json.error==0){
				//$("#t_col1_1").val(weibo_account);
				$("#custom_1").val(json.data.result.screen_name);

				var display_user_info = '<a href="http://weibo.com/'+weibo_account+'" target="_blank" title="'+json.data.result.screen_name+'"><img src="http://tp1.sinaimg.cn/'+weibo_account+'/50/5662076629/1" alt="'+json.data.result.screen_name+'" width="50" height="50" /></a><strong><a href="http://weibo.com/'+weibo_account+'" title="'+json.data.result.screen_name+'" target="_blank">'+json.data.result.screen_name+'</a></strong>';

				$("#user_info_display").html(display_user_info);
			}
		});
	} 
};

//字符串截取
var sub_string = function(str,len)
{  
     var sub_length = len;  
     var temp1 = str.replace(/[^\x00-\xff]/g,"**");
     var temp2 = temp1.substring(0,sub_length);  
     //找出有多少个*  
     var x_length = temp2.split("\*").length - 1 ;  
     var hanzi_num = x_length /2 ;  
     sub_length = sub_length - hanzi_num ;//实际需要sub的长度是总长度-汉字长度  
     var res = str.substring(0,sub_length);    
     return res ;  
}

