<script type="text/javascript">
function form_submit()
{

	if($("#custom_1").val()==''){
		alert('请您填写昵称');
		$("#custom_1").focus();
		return false;
	}
	if($("#email").val()==''){
		alert('请您填写邮箱');
		$("#email").focus();
		return false;
	}
	if($("#custom_2").val()==''){
		alert('请先写点什么吧');
		$("#custom_2").focus();
		return false;
	}

	if (/[a-zA-Z0-9\-_]@[a-zA-Z0-9.]+\.[a-z]{2,4}/.test($("#email").val())) {
		$.JSONP("http://act.city.sina.com.cn/interface/activity/json_add_signup.php?act_id=6570",
			$("#form_div").serialize(),
			function(json)
			{
				if(json.error=="1") {
					alert(json.errmsg);
				}else if(json.error=="0"){
					alert("提交成功！");
					$('#form_div')[0].reset();
				}
			}
			,'jsonp');
	} else {
		alert('邮箱格式不正确。');
		$("#email").focus();
		return false;
	}

}
</script>

