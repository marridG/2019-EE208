$("#weibo_share").live("click", function(e) 
    {

        e.preventDefault();
        HBAPP.Weibo.checkLogin();

                var wb ="如今谣言深似海，一不小心转发500次，后果会是严重滴！你还在为自己被谣言欺骗而懊恼吗？你还在为举报谣言无门而发愁吗？来河南辟谣平台吧，在这里你可以了解最新的辟谣信息，也可以放心举报发现的各种谣言。各位驱谣小伙伴，快来试试吧！http://hebei.sina.com.cn/z/news/piyao/index.shtml";
               HBAPP.Weibo.sendWeibo({
                    content:wb,pic_url:"http://hebei.sinaimg.cn/2014/0526/U7229P1275DT20140526135956.jpg"                  
                }, function(d) { 
                      if(d.error==0) {alert(  " 感谢您的分享!");}
                      else{alert(d.errmsg);}

});
    });

	$("#t_col4_0").keyup(function(){
		var input_content =  $("#t_col4_0").val();

		if(input_content.length > 144){
			alert("您输入的字数超出啦！！");
			$("#t_col4_0").val(sub_string(input_content,144));
		}

		$("#remain_text_num").text( 144 - $("#t_col4_0").val().length ) ;
	});

	$("#search_submit").click(function(){
		var keyword = $(".int_keyword").val();

		if(keyword == '' || keyword == '输入要搜索的关键字'){
			alert("请输入要搜索的关键字！！");
			$(".int_keyword").focus();
			return false;
		}

		$("#search_form").submit(); 
	});
});

