<!-- script section begin -->
<script>
  seajs.use([
    '$',
    'site.factors',
    'site.conditionBlock',
    'nodeSwitch',
    'mSimSelect',
    'mCalendar'
  ], function ($, FactorBlock, ConditionBlock, NodeSwitch, SimSelect, Calendar) {
    // make page data
    var foundTypeBlock = $('#foundType');
    var foundTypeTabContainer = foundTypeBlock.find('.switchTabContainer');
    var foundTypeWrapContainer = foundTypeBlock.find('.switchWrapContainer');
    $.ajax({
      'url': 'http://stock.finance.sina.com.cn/fundfilter/api/openapi.php/MoneyFinanceFundFilterService.getFundTypeList?dpc=1',
      'dataType': 'jsonp',
      'cache': true,
      'jsonpCallback': 'makeTypeData',
      'success': function (obj) {
        if (obj.result.status.code === 0) {
          var dataObj = obj.result.data;
          window.foundType = dataObj;
          $.each(dataObj, function (currentType1Code) {
            var currentType1Name = this.name;
            var currentType2List = this.types;
            var currentTabNode = $('<a class="item listItem" href="javascript:;" title="' + currentType1Name + '" data-switchType="tab">' + currentType1Name + '</a>');
            foundTypeTabContainer.append(currentTabNode);

            /*
             var currentWrapItemAllNode = $('<a class="foundation" href="javascript:;" title="不限" data-apiKey="type1" data-apiValue="' + currentType1Code + '" data-value="' + currentType1Name + ' 不限">不限</a>');
             currentWrapNode.append(currentWrapItemAllNode);
             */
            var currentWrapNode = $('<div class="fct-b-detail fct-b-found udv-clearfix" data-switchType="wrap" style="display: none;"></div>');
            $.each(currentType2List, function (currentType2Code) {
              var currentType2Name = this;
              var currentWrapItemNode = $('<a class="foundation" href="javascript:;" title="' + currentType2Name + '" data-apiKey="type2" data-apiValue="' + currentType2Code + '" data-value="' + currentType1Name + ' ' + currentType2Name + '">' + currentType2Name + '</a>');
              currentWrapNode.append(currentWrapItemNode);
            });
            foundTypeWrapContainer.append(currentWrapNode);
          });

          blockReady();
        } else {
          alert(obj.result.status.msg);
        }
      }
    });

    var foundCompanyBlock = $('#foundCompany');
    var foundCompanyTabContainer = foundCompanyBlock.find('.switchTabContainer');
    var foundCompanyWrapContainer = foundCompanyBlock.find('.switchWrapContainer');
    $.ajax({
      'url': 'http://stock.finance.sina.com.cn/fundfilter/api/openapi.php/MoneyFinanceFundFilterService.getCompanyList?dpc=1',
      'dataType': 'jsonp',
      'cache': true,
      'jsonpCallback': 'makeCompanyData',
      'success': function (obj) {
        if (obj.result.status.code === 0) {
          var dataObj = obj.result.data;
          $.each(dataObj, function (currentFoundIndex) {
            var currentTabNode = $('<a class="item borderItemSmall" href="javascript:;" title="' + currentFoundIndex + '" data-switchType="tab">' + currentFoundIndex + '</a>');
            foundCompanyTabContainer.append(currentTabNode);

            var currentWrapNode = $('<div class="fct-b-detail fct-b-found udv-clearfix" data-switchType="wrap" style="display: none;"></div>');
            $.each(this, function () {
              var currentFoundName = this.cname;
              var currentFoundId = this.cid;
              var currentWrapItemNode = $('<a class="foundation" href="javascript:;" title="' + currentFoundName + '" data-apiValue="' + currentFoundId + '" data-value="' + currentFoundName + '">' + currentFoundName + '</a>');
              currentWrapNode.append(currentWrapItemNode);
            });
            foundCompanyWrapContainer.append(currentWrapNode);
          });

          blockReady();
        } else {
          alert(obj.result.status.msg);
        }
      }
    });

    var foundTopicBlock = $('#topicFilter');
    var foundTopicTabContainer = foundTopicBlock.find('.switchTabContainer');
    var foundTopicContainer = foundTopicBlock.find('.switchWrapContainer');
    $.ajax({
      'url': 'http://stock.finance.sina.com.cn/fundfilter/api/openapi.php/MoneyFinanceFundFilterService.getSubjectList?dpc=1',
      'dataType': 'jsonp',
      'cache': true,
      'jsonpCallback': 'makeSubjectData',
      'success': function (obj) {
        if (obj.result.status.code === 0) {
          var dataObj = obj.result.data;
          var moreNode = $('<a class="item listItem" href="javascript:;" title="更多主题" data-switchType="tab">更多主题</a>');
          var wrapNode = $('<div class="fct-b-detail fct-b-found udv-clearfix" data-switchType="wrap" style="display: none;"></div>');
          $.each(dataObj, function (i) {
            var stName = this.sname;
            var stCode = this.stype;
            if (i < 9) {
              var currentTabNode = $('<a class="item borderItem" href="javascript:;" title="' + stName + '" data-apiValue="' + stCode + '" data-value="' + stName + '">' + stName + '</a>');
              foundTopicTabContainer.append(currentTabNode);
            } else {
              var currentItemNode = $('<a class="foundation" href="javascript:;" title="' + stName + '" data-apiValue="' + stCode + '" data-value="' + stName + '">' + stName + '</a>');
              wrapNode.append(currentItemNode);
            }
          });
          foundTopicTabContainer.append(moreNode);
          foundTopicContainer.append(wrapNode);

          blockReady();
        } else {
          alert(obj.result.status.msg);
        }
      }
    });

    // page init
    var dynamicBlockAmount = 3;
    var readyBlockAmount = 0;

    function blockReady() {
      if (++readyBlockAmount === dynamicBlockAmount) {
        pageInit();
      }
    }

    function pageInit() {
      // condition block init
      var cdBlock = new ConditionBlock({
        'containerNode': $('#conditionBlock')
      });

      // filter block init
      var currentBlock, currentSimSelectNodeList, currentDateNodeList;

      // hot filter block
      currentBlock = $('#hotFilter');
      currentBlock.delegate('a', 'click', function () {
        setTimeout(function () {
          window.location.reload();
        }, 10);
      });

      /*
       var fbHotFilter = new FactorBlock({
       'node': currentBlock[0],
       'isSingle': true,
       'conditionBlock': cdBlock
       });
       */

      // fonud type block
      currentBlock = foundTypeBlock;
      var nsFoundType = new NodeSwitch({
        'containerId': 'foundType',
        'selectedClass': 'switchSelected',
        'eventType': 'click',
        'canCancel': true,
        'autoShow': false
      });
      var fbFoundType = new FactorBlock({
        'node': currentBlock[0],
        'isSingle': false,
        'nodeSwitchObj': nsFoundType,
        'conditionBlock': cdBlock
      });

      // rank filter block
      currentBlock = $('#rankFilter');
      var nsRankFilter = new NodeSwitch({
        'containerId': 'rankFilter',
        'selectedClass': 'switchSelected',
        'eventType': 'click',
        'canCancel': true,
        'autoShow': false
      });
      var fbRankFilter = new FactorBlock({
        'node': currentBlock[0],
        'isSingle': false,
        'switchSingle': true,
        'nodeSwitchObj': nsRankFilter,
        'conditionBlock': cdBlock
      });

      // found company block
      currentBlock = foundCompanyBlock;
      var nsFoundCompany = new NodeSwitch({
        'containerId': 'foundCompany',
        'selectedClass': 'switchSelectedSmall',
        'canCancel': false,
        'autoShow': false
      });
      var fbFoundCompany = new FactorBlock({
        'node': currentBlock[0],
        'isSingle': false,
        'nodeSwitchObj': nsFoundCompany,
        'conditionBlock': cdBlock
      });

      // topic filter block
      currentBlock = foundTopicBlock;
      var nsTopicFilter = new NodeSwitch({
        'containerId': 'topicFilter',
        'selectedClass': 'switchSelected',
        'eventType': 'click',
        'canCancel': true,
        'autoShow': false
      });
      var fbTopicFilter = new FactorBlock({
        'node': currentBlock[0],
        'isSingle': false,
        'nodeSwitchObj': nsTopicFilter,
        'conditionBlock': cdBlock
      });

      // manager filter block
      currentBlock = $('#managerFilter');
      var fbManagerFilter = new FactorBlock({
        'node': currentBlock[0],
        'isSingle': false,
        'conditionBlock': cdBlock
      });

      // money filter block
      currentBlock = $('#moneyFilter');
      var fbMoneyFilter = new FactorBlock({
        'node': currentBlock[0],
        'isSingle': false,
        'conditionBlock': cdBlock
      });

      // time filter block
      currentBlock = $('#timeFilter');
      var fbTimeFilter = new FactorBlock({
        'node': currentBlock[0],
        'isSingle': false,
        'conditionBlock': cdBlock
      });

      // star filter block
      currentBlock = $('#starFilter');
      currentSimSelectNodeList = currentBlock.find('[data-nodeType="mSimSelect"]');
      var simSelectList = [];
      $.each(currentSimSelectNodeList, function () {
        var tempSelect = new SimSelect({
          'node': this
        });
        simSelectList.push(tempSelect);
      });
      var fbStarFilter = new FactorBlock({
        'node': currentBlock[0],
        'isSingle': false,
        'simSelectList': simSelectList,
        'conditionBlock': cdBlock
      });
    }

    // block fold
    var isFolding = true;
    var blockFoldBtn = $('#blockFoldBtn');
    var foldBlockIdList = ['topicFilter', 'managerFilter', 'moneyFilter', 'timeFilter', 'starFilter'];
    blockFoldBtn.click(function () {
      if (isFolding) {
        isFolding = false;
        blockFoldBtn.find('span').html('收起');
        blockFoldBtn
            .attr('title', '收起')
            .addClass('fct-hide')
            .removeClass('fct-show');
        $.each(foldBlockIdList, function () {
          $('#' + this).css('display', 'block');
        });
      } else {
        isFolding = true;
        blockFoldBtn.find('span').html('高级筛选条件');
        blockFoldBtn
            .attr('title', '高级筛选条件')
            .addClass('fct-show')
            .removeClass('fct-hide');
        $.each(foldBlockIdList, function () {
          $('#' + this).css('display', 'none');
        });
      }
    });

    // compare block
    var compareShowBtn = $('#compareShowBtn');
    var compareHideBtn = $('#compareHideBtn');
    var compareWrap = $('.compare-wrap');
    var isCompareFolding = true;
    var userFolding = true;

    compareShowBtn.click(function () {
      foldCompare(false, false);
    });
    compareHideBtn.click(function () {
      foldCompare(true, false);
    });

    $(window).bind('scroll resize', function () {
      var currentTopSpace = $('#resultBlock')[0].getBoundingClientRect().top;
      var maxTopSpace = ($(window).height() - compareWrap.height()) / 2;
      if (currentTopSpace >= maxTopSpace) {
        foldCompare(true, true);
      } else if (!userFolding) {
        foldCompare(false, true);
      }
    });

    window.foldCompare = function (isFold, ifScrollFold) {
      if (isFold) {
        compareWrap.css('display', 'none');
        compareShowBtn.css('display', 'block');
        isCompareFolding = true;
        if (!ifScrollFold) {
          userFolding = true;
        }
      } else {
        compareWrap.css('display', 'block');
        compareShowBtn.css('display', 'none');
        isCompareFolding = false;
        if (!ifScrollFold) {
          userFolding = false;
        }
      }
    };

    /*
     new NodeSwitch({
     'containerId': 'foundCompany',
     'selectedClass': 'switchSelected'
     });

     new NodeSwitch({
     'containerId': 'foundType',
     'selectedClass': 'switchSelected',
     'eventType': 'click'
     });

     // init factors block
     var factorBlockList = $('.fct-block');
     $.each(factorBlockList, function () {
     var currentBlock = $(this);
     var currentSimSelectNodeList = currentBlock.find('[data-nodeType="mSimSelect"]');
     var currentDateNodeList = currentBlock.find('[data-nodeType="date"]');
     var simSelectList = [];
     var dateList = [];

     // init sim select
     $.each(currentSimSelectNodeList, function () {
     var tempSelect = new SimSelect({
     'node': this
     });
     simSelectList.push(tempSelect);
     });

     // init date viewer
     $.each(currentDateNodeList, function () {
     var tempDate = new Calendar({
     'elem': this,
     'startDate': '2013-1-1',
     'selectDate': ''
     });
     dateList.push(tempDate);
     });

     new FactorBlock({
     'node': this,
     'isSingle': false,
     'simSelectList': simSelectList,
     'dateList': dateList,
     'conditionBlock': cdBlock
     });
     });
     */
  });
</script>
<!-- script section end -->
