.weibo-list-a .name:link{color:#333;text-decoration: none;}
.weibo-list-a .name:visited{color:#666;text-decoration: none;}
.weibo-list-a .name:hover, .weibo-list-a .name:active{color:#0077dd;text-decoration: none;}
.weibo-list-a .txt a{position: relative;display: block;width:100%;height: 100%;}
.weibo-list-a .txt a:link{color:#666;text-decoration: none;}
.weibo-list-a .txt a:visited{color:#888;text-decoration: none;}
.weibo-list-a .txt a:hover, .weibo-list-a .txt a:active{color:#0077dd;text-decoration: none;}
