.scrollpanel-a .jspHorizontalBar { position: absolute; bottom: 0; left: 0; width: 100%; height: 6px; background: #fff; }
.scrollpanel-a .jspCap { display: none; }
.scrollpanel-a .jspHorizontalBar .jspCap { float: left; }
.scrollpanel-a .jspTrack { background: #EBEBEB; position: relative;border-radius: 4px; }
.scrollpanel-a .jspDrag { background: #D0D0D0; position: relative; top: 0; left: 0; cursor: pointer; border-radius: 4px;}
.scrollpanel-a .jspHover{background: #ccc;}
.scrollpanel-a .jspHorizontalBar .jspTrack,
.scrollpanel-a .jspHorizontalBar .jspDrag { float: left; height: 100%; }
.scrollpanel-a .jspArrow { background: #50506d; text-indent: -20000px; display: block; cursor: pointer; padding: 0; margin: 0; }
.scrollpanel-a .jspArrow.jspDisabled { cursor: default; background: #80808d; }
.scrollpanel-a .jspVerticalBar .jspArrow { height: 6px; }
.scrollpanel-a .jspHorizontalBar .jspArrow { width: 6px; float: left; height: 100%; }
.scrollpanel-a .jspVerticalBar .jspArrow:focus { outline: none; }
.scrollpanel-a .jspCorner { background: #eeeef4; float: left; height: 100%; }

