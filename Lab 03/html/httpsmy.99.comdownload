<script language="javascript">
    var getUrl = window.location.href;
    var arrUrl = new Array();
    arrUrl = getUrl.split("=")
    if (arrUrl.length > 1) {
        var getUrlID = parseInt(arrUrl[1]) + 1
        if (getUrlID > 6) {
            setTab('down', 1, 2)
        }
        else {
            setTab('down', getUrlID, 2)
        }
    }
    //滚动
    $.ajax({
        type: "get",
        async: false,
        url: "https://xfqb.my.99.com/?r=/index/loadNewServer&bjsonp=1",
        dataType: "jsonp",
        scriptCharset: "utf-8",
        success: function (data) {
            var html = '';
            for (var i = 0; i < data.list.length; i++) {
                html += "<p>" + data.list[i].open_d + "\n" + data.list[i].open_time + data.list[i].area + data.list[i].server + "</p>"
            }
            document.getElementById('marquetd1').innerHTML = html;
        },
        error: function () {
            //alert("没有新区服")
        }
    });
    $(function () {
//单行应用@Mr.Think
        var _wrap = $('#marquetd1');//定义滚动区域
        var _interval = 3000;//定义滚动间隙时间
        var _moving;//需要清除的动画
        _wrap.hover(function () {
            clearInterval(_moving);//当鼠标在滚动区域中时，停止滚动
        }, function () {
            _moving = setInterval(function () {
                var _field = _wrap.find('p:first');//此变量不可放置于函数起始处，li:first取值是变化的
                var _h = _field.height();//取得每次滚动高度
                _field.animate({marginTop: -_h + 'px'}, 600, function () {//通过取负margin值，隐藏第一行
                    _field.css('marginTop', 0).appendTo(_wrap);//隐藏后，将该行的margin值置零，并插入到最后，实现无缝滚动
                })
            }, _interval)//滚动间隔时间取决于_interval
        }).trigger('mouseleave');//函数载入时，模拟执行mouseleave，即自动滚动
    });
</script>
