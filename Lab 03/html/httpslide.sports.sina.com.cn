    <script type="text/javascript">
    ;(function() {
	    var strSplit2 = function (s, n) {
            var ELLIPSIS = '...';
            var s2 = s.slice(0, n),
                i = s2.replace(/[^\x00-\xff]/g, "**").length,
                j = s.length,
                k = s2.length;
            if(i<n){
                return s2;
            }else if(i==n){
                if(n==j||k==j){
                    return s2;
                }else{
                    return s.slice(0,n-2)+ELLIPSIS;
                }
            }
            i -= s2.length;
            switch (i) {
                case 0: return s2;
                case n:
                    var s4;
                    if(n==j){
                        s4 = s.slice(0, (n>>1)-1);
                        return s4+ELLIPSIS;
                    }else{
                        s4 = s.slice(0, n>>1);
                        return s4;
                    }
                default:
                    var k = n - i,
                        s3 = s.slice(k, n),
                        j = s3.replace(/[\x00-\xff]/g, "").length;
                    return j ? s.slice(0, k) + arguments.callee(s3, j) : s.slice(0, k);
            }
        };
        // 导航
        var nav = $('#SI_Nav');
        nav.on('mousemove','.list', function() {
            var item = $(this);
            var list = item.find('dl');
            list.width(item.outerWidth());
            item.addClass('hover');
        });
        nav.on('mouseleave','.list', function() {
            var item = $(this);
            item.removeClass('hover');
        });

        // 主要数据加载
        var handleData = (function(){
            // 委派事件只绑定一次
            var app = PhotoBase.app;
            return function(wrap){
                app.renderLikeNum(wrap);
                app.renderCmntNum(wrap);
                // app.bindLikeNum(wrap,'voted');
            };
        })();
        var render = function(data,self){
            data = data.data;
            var items = self.get('dom.items');
            var html = [];
            var strLeft = PhotoBase.util.strLeft;
            var renderItem = function(item,hackClz){
                var url = item.url;
                var time = item.createtime.substr(5,11).replace('-','/');
                //var img = item.img_198_132||'http://i1.sinaimg.cn/home/main/blk/d.gif';
                var img = 'http://s.img.mix.sina.com.cn/auto/resize?size=235_156&img='+item.img_url;
                if(item.cover_ori_3_2 != '') {
                    img = 'http://s.img.mix.sina.com.cn/auto/resize?size=235_156&img='+item.cover_ori_3_2;
                }
                var albumname = item.name;
                if(item.short_name != '') {
                    albumname = item.short_name;
                }
                var fileType = item.img_url.substring(item.img_url.length-3);/*0409 zyc */
                var gifHtml = fileType.toLowerCase()=='gif'?'<div class="gif"><img src="/images/v1/default/GIF.gif"/></div>':'';/*0409 zyc */
                return '<div class="item '+hackClz+'">'
                    +'<div class="bg"></div>'
                    +'<div class="hd">'
                        +'<a target="_blank" href="'+url+'"><img src="'+img+'" alt="'+item.short_name+'"></a>'
                    +'</div>'
                    +'<div class="bd">'
                        +'<h3><a href="'+url+'" title="'+item.name+'">'+albumname+'</a></h3>'
                        +'<p>'+strSplit2(item.short_intro,96)+'</p>'
                    +'</div>'
                    +'<div class="ft"><span class="time"><i></i>'+time+'</span><a node-type="smallCmnt" action-data="'+item.cmnt_ch+'_slidenews-album-'+item.sid+'-'+item.id+'" title="评论"  href="'+url+'#J_Comment_Wrap" class="comment"><i></i>0</a><span node-type="smallLike" action-data="'+item.cmnt_ch+'_slidenews-album-'+item.sid+'-'+item.id+'" title="喜欢" class="like"><i></i>0</span></div>'
                    +gifHtml
                +'</div>';
            };
            var len = data.length;

            // 没有数据
            if(len==0){
                return;
            }
            for (var i = 0; i < len; i++) {
                var item = data[i];
                var hackClz = 'item-bottom';
                if(i+4<len){
                    hackClz = '';
                }
                if(i==len-1||((i+1)%4==0)){
                    hackClz = hackClz+' '+'item-right';
                }
                html.push(renderItem(item,hackClz));
            }
            items.hide();
            items.html(html.join('')).fadeIn('slow');
            // 顶数，评论数，顶操作，简介收展
            var wrap = self.get('dom.wrap');
            handleData(wrap);
        };
		
		
