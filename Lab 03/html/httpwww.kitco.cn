        //返回0的话，说明两个日期格式有问题  1:第一个日期大于第二个日期  2.第1个日期小于第2个日期 3.两个日期相等
        function compareDate(startYear, startMonth, startDay, endYear, endMonth, endDay) {

            if (Date.parse(startMonth + "/" + startDay + "/" + startYear) > Date.parse(endMonth + "/" + endDay + "/" + endYear)) {
                return 1;
            } else if (Date.parse(startMonth + "/" + startDay + "/" + startYear) < Date.parse(endMonth + "/" + endDay + "/" + endYear)) {
                return 2;
            } else if ((Date.parse(startMonth + "/" + startDay + "/" + startYear) = Date.parse(endMonth + "/" + endDay + "/" + endYear))) {
                return 3;
            }

            return 0;
        }

