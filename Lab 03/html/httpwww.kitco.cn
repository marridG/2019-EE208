        function RegMatch(regEx, value) {
            this._regEx = regEx;
            this._value = value;
        }
        RegMatch.prototype.doMatch = function() {
            var re = new RegExp(this._regEx);

            return this._value.search(re) == 0;
        }

        function UpdatePrice(url, oper) {
            xmlHttp1 = getXmlHttpObject();
            if (xmlHttp1 == null) {
                alert("Your browser does not support AJAX!");
                return;
            }
            if (oper == "ConvertMetalPrice") {
                xmlHttp1.onreadystatechange = this.stateChangedConvertGoldPrice;
            } else
                xmlHttp1.onreadystatechange = this.stateChangedConvertExch;
            xmlHttp1.open("GET", url, true);
            xmlHttp1.send(null);
        }


        function stateChangedConvertGoldPrice() {
            if (xmlHttp1.readyState == 4) {
                var tmp = xmlHttp1.responseText;
                document.getElementById("text_gold_dest").value = tmp;
            }
        }

        function stateChangedConvertExch() {
            if (xmlHttp1.readyState == 4) {
                var tmp = xmlHttp1.responseText;
                document.getElementById("text_cur_dest").value = tmp;
            }
        }


        function ConvertPrice(textSource, selSource, textDest, selDest, oper) {
            var oTextSrc = document.getElementById(textSource);
            var oSelSrc = document.getElementById(selSource);
            var oTextDest = document.getElementById(textDest);
            var oSelDest = document.getElementById(selDest);
            var value = oTextSrc.value;
            var regMatch = null;
            var match = null;

            if (value.length > 0) {
                regMatch = new RegMatch("[-+]?[0-9]*\\.?[0-9]*$", value);
                match = regMatch.doMatch();

                if (!match) {
                    alert("请输入一个有效数字");
                    oTextSrc.focus();
                    oTextSrc.select();
                } else {
                    var requestLink = "/KitcoDynamicSite/RequestHandler?requestName=ConvertPrice&OperationType=" + oper + "&From=" + oSelSrc.value + "&To=" + oSelDest.value + "&Value=" + oTextSrc.value;

                    UpdatePrice(requestLink, oper);

                }
            }
        }
