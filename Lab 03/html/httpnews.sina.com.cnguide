<script>
(function(){
    var lab = document.getElementById('XX_labs'),
        labs = lab.getElementsByTagName('a'),
        labl = labs.length,
        cname = 'selected',
        animation = false;
    FN.delegate(lab,'XX_lab','click',function(e,t){
        if(animation){return;}
        animation = true;
        var did = t.getAttribute('did'),
            tar = document.getElementById(did),
            index = 0,
            counter = 0,
            cont = document.getElementById('XX_conts'),
            conts = FN.getByClass('section',cont,'div'),
            contl = conts.length;
        if(!tar){
            animation = false;
            return;
        }
        for(var i = 0; i < labl; i++){
            FN.removeClass(labs[i],cname);
        }
        FN.addClass(t,cname);
        for(var i = 0; i < contl; i++){
            if(conts[i] === tar){
                break;
            }
            index++;
        }
        if(index === 0){
            animation = false;
            return;
        }
        for(var i = 0; i < index; i++){
            var ele = conts[i],
                during = 500 + i;
            (function(ele,during){
                FN.slideUp(ele,during,function(){
                    cont.appendChild(ele);
                    ele.style.height = '';
                    ele.style.display = '';
                    ele.style.overflow = '';
                    counter++;
                    if(counter === index){
                        animation = false;
                    }
                })
            })(ele,during);
        }
    });
    FN.addClass(labs[0],cname);
})();
