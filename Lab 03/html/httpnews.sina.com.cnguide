(function(){
    var k = document.getElementById('search_k'),
        f = document.getElementById('search_f'),
        sv = k.value,
        oc = [],
        cname = 'marked';
    FN.addEvent(f,'submit',function(e){
        FN.preventDefault(e);
        for(var i = 0, l = oc.length; i < l; i++){
            FN.removeClass(oc[i],cname,0);
        }
        var v = k.value.toLowerCase(),
            c = document.getElementsByTagName('*'),
            sc = [];
        if(v === '' || v === sv){return;}
        for(var i = 0, l = c.length; i < l; i++){
            var ci = c[i],
                ch = ci.innerHTML;
            if(c[i].nodeType === 1 && ch.toLowerCase().indexOf(v) !== -1 && ci.getElementsByTagName('*').length === 0){
                sc.push(ci);
            }
        }
        for(var i = 0, l = sc.length; i < l; i++){
            FN.addClass(sc[i],cname);
        }
        if(sc.length === 0){return;}
        FN.scrollIn(sc[0],500,100);
        oc = sc;
    });
    FN.addEvent(document.body,'click',function(e){
        for(var i = 0, l = oc.length; i < l; i++){
            FN.removeClass(oc[i],cname,0);
        }
    });
})();
</script>



