<script>
function wbShare(url,text,picurl) {
	var temp = 'http://v.t.sina.com.cn/share/share.php?title=%title%&url=%url%&ralateUid=1271638160&source=%source%&sourceUrl=http://sports.sina.com.cn/video/&pic=%picurl%';
	var href = window.location.href.split("#")[0];
	var title = encodeURIComponent('我正在看【中国足球超级联赛】' + href + ' 视频专题的“' + text + '”');
	var source="新浪体育视频";
	var pic=picurl||"";
	var paras = temp.replace(/%title%/g,title).replace(/%url%/g,url).replace(/%source%/g,source).replace(/%picurl%/g,pic);
	window.open(paras,'_blank','width=600,height=500');
}
</script>
