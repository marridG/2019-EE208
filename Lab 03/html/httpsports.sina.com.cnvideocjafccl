<script>
function setCmtCount(datas){
	var vcount = getById('video_cmt_count');
	if(!vcount){return;}
	vcount.innerHTML = '';
	if(datas.result.status.code == 0){
		vcount.innerHTML = datas.result.count[document.getElementById('cmtjsonscript').getAttribute('data-cid')].total;
	}
}
</script>
