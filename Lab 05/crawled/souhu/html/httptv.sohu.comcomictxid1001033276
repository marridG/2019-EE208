                    kao('jquery', function() {
                      $(window).scroll(function(){
                          sc=$(window).scrollTop();
                          hsc=$(".hd-focus").outerHeight();
                          if(sc>hsc){
                              $(".hd-topbar").addClass("hd-topbar-shadow").show();
                          }else{
                              $(".hd-topbar").removeClass("hd-topbar-shadow")
                          }
                      });
                      if(category && category ==='261789193' && $('.hd-nav .item').eq(1).text() == '电视剧'){  //电视剧
                        $('.hd-nav .item').removeClass('item-on');
                        $('.hd-nav .item').eq(1).addClass('item-on');
