	<script>
		;(function () {
			var doc = document.documentElement
			if (!doc) return
			var $html = $('html')
			var $sections = $('.wrapper_page>.section')
			var timer
			var rects
			function getScrollTop() {
			    return window.pageYOffset || document.documentElement.scrollTop || document.body.scrollTop
			}
			function getVisibleView(rect) {
				// 70 是导航条的高度
				var t = rect.top - 70 - getScrollTop()
				var b = rect.bottom - 70 - getScrollTop()
				var cHeight = doc.clientHeight - 70
				if (t <= 0 && b <= 0) {
					return 0
				} else if (t <= 0 && b > 0) {
					return b
				} else if (t >0 && b > 0) {
					if (t >= cHeight) return 0
					else return cHeight - t
				} else {
					return 0
				}
			}
			window._calcSectionBound = function (index) {
				if (!rects) {
					rects = []
					$sections.each(function (i, el) {
						var rect = el.getBoundingClientRect()
						// 相对计算
						rects.push({
							top: rect.top + getScrollTop(),
							bottom: rect.bottom + getScrollTop()
						})
					})
				}
				clearTimeout(timer)
				timer = setTimeout(function () {
					rects = null
				}, 300)
				var tarIndex = 0
				var tarView = getVisibleView(rects[tarIndex])
				// 寻找可视区域最大的

				for (var i = 1; i < rects.length; i++) {
					var cur = getVisibleView(rects[i])
					if (cur > tarView) {
						tarIndex = i
						tarView = cur
					}
				}
				return tarIndex === index
			}
			function _handler() {
				var width = doc.clientWidth
				var height = doc.clientHeight

				if ($html.hasClass('lte_ie8')) {
					if (width <= 1279) {
						$html.addClass('screen_xs')
							.removeClass('screen_ms')
					} else if (width <= 1599) {
						$html.addClass('screen_ms')
							.removeClass('screen_xs')
					} else {
						$html.removeClass('screen_xs')
							.removeClass('screen_ms')
					}
				}

				if (height < 700) {
					$('body').addClass('screen_h_ms')
				}
			}
			$(window).on('resize', _handler)
			_handler()
		})()
	</script>
