            os = 'os_winxp';
        }
        if(ua.indexOf('Windows NT')>-1){
            os = 'os_win';
        }else if(ua.indexOf('iPhone')>-1 || ua.indexOf('iPod')>-1 || ua.indexOf('iPad')>-1){
            os = 'os_ios';
        }else if(ua.indexOf('Mac OS X')>-1){
            os = 'os_mac';
        }

        document.getElementsByTagName('html')[0].className += ' '+os;


        // ie8一下的浏览器需要根据宽屏、窄屏添加不同类名，小于 1540 的则进入窄屏模式
        var window_width;
        if( $('html').hasClass('lte_ie8') ){
            windows_width();
            function windows_width(){
                window_width = document.documentElement.clientWidth;
                if( window_width < 1540 ){
                    $('html').addClass('screen_xs');
                }else{
                    $('html').removeClass('screen_xs');
                }
            }
            $(window).resize(function(){
                windows_width();
            });
        }
    </script>
