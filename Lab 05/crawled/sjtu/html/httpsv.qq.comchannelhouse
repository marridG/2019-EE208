        page: window.channelName,
        channel: function (t) {
            return t && t.el && t.el.parents(".mod_row_box").attr("id");
        },
    });
    window.guangping_crystal && window.guangping_crystal();
    window.shortcut && window.shortcut.init({
        download: true, // 下载
        top: true, // 回到顶部
        usersign: true, // 签到
        feedback: true // 意见反馈
    })
</script>
</body>

</html>