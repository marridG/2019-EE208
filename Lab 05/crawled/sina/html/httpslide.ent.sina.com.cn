                j = s.length,
                k = s2.length;
            if(i<n){
                return s2;
            }else if(i==n){
                if(n==j||k==j){
                    return s2;
                }else{
                    return s.slice(0,n-2)+ELLIPSIS;
                }
            }
            i -= s2.length;
            switch (i) {
                case 0: return s2;
                case n:
                    var s4;
                    if(n==j){
                        s4 = s.slice(0, (n>>1)-1);
                        return s4+ELLIPSIS;
                    }else{
                        s4 = s.slice(0, n>>1);
                        return s4;
                    }
                default:
                    var k = n - i,
                        s3 = s.slice(k, n),
                        j = s3.replace(/[\x00-\xff]/g, "").length;
