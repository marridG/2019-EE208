	var city = 'sh';
	var level1_page = 'act';
	var level2_page = 'index';
	var level3_page = '';
	var custom_id = '';

	function tongji(tname,loc_source,news_source,custom_id) {

		if (typeof lejuTj == 'undefined')
		{
			return ;
		}
		if(!news_source){news_source = ''}
		if(!custom_id){custom_id = ''}
		var event_params = new Object();
		event_params.bid = '8';//必填固定
		event_params.city_en = 'sh';//传city_en
		event_params.level1_page = 'act';//传level1_page
		event_params.level2_page ='index';//传level2_page
		event_params.level3_page = '';//传level3_page
		event_params.custom_id = custom_id;//传活动id（无则不传）
		event_params.loc_source =loc_source;//传左侧对应值
		event_params.news_source =news_source;//传左侧对应值
		//调用推送方法
		lejuTj.event(tname, event_params);//替换事件名称
	}
