        //     url: submitUrl,
        //     data: {},
        //     dataType: "json",
        //     success: function(data){
        //       console.log(data);
        //     }
        // });

        var jsonp = function(url, callback, callbackName) {
            if (!url) {
                return;
            }
            if (url.indexOf('?') === -1) {
                url += '?callback=';
            } else {
                url += '&callback=';
            }
            url += callbackName;
            var script = document.createElement('script');
            window[callbackName] = function(data) {
                callback && callback(data);
            }
            script.src = url;
            document.getElementsByTagName('head')[0].appendChild(script);
        };

        // 设为默认按钮
        var $dcContainer = $("#slider_dc_container");
        var $dc = $("#slider_dc");
        var $dcBox = $("#slider_dc_box");
        var $dcName = $("#sdcb_name");
        var left = $("#slider_ct_name").outerWidth(true);
        $dcContainer.css({"left": left + "px"});

        // 进入第一次先判断是否有默认城市
        function getCookie (name) {
            var start = document.cookie.indexOf(name + "=");
            var len = start + name.length + 1;
            if ((!start) && (name != document.cookie.substring(0, name.length))) {
                return null;
            }
            if (start == -1) return null;
            var end = document.cookie.indexOf(';', len);
            if (end == -1) end = document.cookie.length;
            return decodeURIComponent( document.cookie.substring(len, end));

        }
        function setNameWidth(){
            var fnum = $dcName.text().length;
            var wid = 18*fnum;
            $dcName.width(wid);
            $dcBox.width(wid+84);
        }
        setNameWidth();
        var loc_info = getCookie('loc_info') || "";
        var arrLocInfo = !!loc_info ? loc_info.split(";") : [];
        if( !!arrLocInfo.length && !!arrLocInfo[0] && !!arrLocInfo[1] && !!arrLocInfo[2] ){
            $dc.addClass("hasSet");
            var defaulturl = arrLocInfo[1];
            var defaultname = decodeURIComponent(arrLocInfo[2]);
            $dcName.attr("href",defaulturl);
            $dcName.html(defaultname);
            setNameWidth();
        }

        $dcContainer.on("mouseenter",function(){
            if( $dc.hasClass('hasSet') ){
                $dcBox.addClass("show");
            }
        });
        $dcContainer.on("mouseleave",function(){
            if( $dc.hasClass('hasSet') ){
                $dcBox.removeClass("show");
            }
        });
        // 设为默认
        $dc.on("click",function(){
            var $that = $(this);
            if( !$that.hasClass('hasSet') ){
                _postCityCode (citycode);
                return false;
            }
        });
//        function delCookie(name)
//               {
//                   var exp = new Date();
//                   exp.setTime(exp.getTime() - 1);
//                   var cval=getCookie(name);
//                   if(cval!=null)
//                       document.cookie= name + "="+cval+";expires="+exp.toGMTString();
//               }
               // 删除默认
               var $btn_del = $("#sdcb_del");
               $btn_del.on("click",function(){
                    delLocal('loc_info','/','weather.sina.com.cn');
//                   delCookie('loc_info');
                   $dc.removeClass("hasSet");
                   $dcBox.removeClass("show");
                   return false;
               });

        // 修改默认城市
        var $citiesLayer;
        // 正在请求城市接口ajax标识锁
        var citiesAddRQ = false;
        // 添加城市按钮
        $.subscribe('addDefaultCity', function () {
            $citiesLayer = $('#slider_add_c_dc');
            if(!$citiesLayer.length){
                $citiesLayer = $('\
                    <div class="slider_add_c" id="slider_add_c_dc">\
                        <span class="slider_add_close" id="slider_add_close"></span>\
                        <h4 class="slider_add_tt slider_add_tt0">\u4fee\u6539\u9ed8\u8ba4\u57ce\u5e02</h4>\
                        <h4 class="slider_add_tt slider_add_tt1" style="display: none;">\u8BF7\u9009\u62E9\u57CE\u5E02</h4>\
                        <input type="text" name="" class="slider_add_input" id="slider_add_input" placeholder="\u8BF7\u8F93\u5165\u57CE\u5E02\u540D">\
                        <p class="slider_add_none" style="display: none;">\u672A\u627E\u5230\u5339\u914D\u7684\u57CE\u5E02</p>\
                        <div class="slider_add_btns clearfix">\
                            <span class="slider_add_btn slider_add_btn_cancel" id="slider_add_btn_cancel">\u53D6\u6D88</span>\
                            <span class="slider_add_btn slider_add_btn_add" id="slider_add_btn_add">\u786E\u5B9A</span>\
                        </div>\
                        <div class="slider_add_ctlist clearfix" style="display: none;"></div>\
                    </div>');
                $citiesLayer.on('click', '.slider_add_close', function (e) {
                    _hideCitiesLayer();
                });
                $citiesLayer.on('click', '.slider_add_btn_cancel', function (e) {
                    _hideCitiesLayer();
                });
                // 低版本浏览器加入输入提示
                if(!_attrSupportCk('placeholder', 'input')){
                    $citiesLayer.find('.slider_add_input').on('focus', function () {
                        if($(this).val() == '\u8BF7\u8F93\u5165\u57CE\u5E02\u540D'){
                            $(this).val('');
                        }
                    });
                    $citiesLayer.find('.slider_add_input').on('blur', function () {
                        if($(this).val() == ''){
                            $(this).val('\u8BF7\u8F93\u5165\u57CE\u5E02\u540D');
                        }
                    });
                }
                $citiesLayer.on('click', '.slider_add_btn_add', function (e) {
                    var suggestAPI = 'http://open.weather.sina.com.cn/api/location/getIndexSuggestion/';
                    var sgUrl;
                    var $slider_add_input = $('.slider_add_input').eq(0);
                    if(citiesAddRQ){
                        return;
                    }
                    // 输入字节小于2不允许搜索
                    if($.trim($slider_add_input.val()).replace(/([^\x00-\xff])/g, "\x00$1").length < 2){
                        alert('\u8BF7\u8F93\u5165\u57CE\u5E02\u540D\u79F0');
                        $slider_add_input.get(0).focus();
                        return;
                    }
                    _citiesLayerLoad();
                    sgUrl = suggestAPI + $.trim($slider_add_input.val());
                    citiesAddRQ = true;
                    $.ajax({
                        dataType : "jsonp",
                        url : sgUrl,
                        data : '',
                        cache : true,
                        jsonpCallback : 't' + Math.floor(Date.parse(new Date())/100000),
                        success : function (d) {
                            var data_ctlist = d.result.data;
                            var ctlistLen = data_ctlist.length;
                            var ctlistStr = '';
                            // 如果仅有一个结果,且这个结果与输入一致, 直接添加即可
                            if(ctlistLen == 1 && (data_ctlist[0].chinese_name == $slider_add_input.val() || data_ctlist[0].name == $slider_add_input.val())){
                                _hideCitiesLayer();
                                _postCityCode(data_ctlist[0].loc_code);
                            }
                            // 没有匹配的结果
                            else if(ctlistLen == 0){
                                if($('.slider_add_none').length){
                                    $('.slider_add_btns').hide(0);
                                    $('.slider_add_none').fadeIn(300);
                                    setTimeout(function () {
                                        if($('.slider_add_none').length && $('.slider_add_btns').length){
                                            $('.slider_add_btns').show(0);
                                            $('.slider_add_none').hide(0);
                                        }
                                    }, 2000);
                                }
                            }
                            else{
                                for(var i = 0; i < ctlistLen; i ++){
                                    ctlistStr += ('\
                                        <span class="slider_add_ctlist_i" data-citycode="' + data_ctlist[i].loc_code + '">' + (data_ctlist[i].chinese_name == '' ? data_ctlist[i].name : data_ctlist[i].chinese_name)  + '</span>\
                                    ');
                                    $('.slider_add_ctlist').html(ctlistStr);
                                }
                                _setCitiesLayerStates(1, true);
                                // _renderCitiesList(data_ctlist);
                            }
                            citiesAddRQ = false;
                        }
                    });
                });
                // 键盘回车搜索
                $citiesLayer.on('keydown', function (e) {
                    var keyCode = e.keyCode;
                    var suggestAPI = 'http://open.weather.sina.com.cn/api/location/getIndexSuggestion/';
                    var sgUrl;
                    var $slider_add_input = $('.slider_add_input').eq(0);
                    if(citiesAddRQ || keyCode != 13){
                        return;
                    }
                    // 输入字节小于2不允许搜索
                    if($.trim($slider_add_input.val()).replace(/([^\x00-\xff])/g, "\x00$1").length < 2){
                        alert('\u8BF7\u8F93\u5165\u57CE\u5E02\u540D\u79F0');
                        $slider_add_input.get(0).focus();
                        return;
                    }
                    _citiesLayerLoad();
                    sgUrl = suggestAPI + $.trim($slider_add_input.val());
                    citiesAddRQ = true;
                    $.ajax({
                        dataType : "jsonp",
                        url : sgUrl,
                        data : '',
                        cache : true,
                        jsonpCallback : 't' + Math.floor(Date.parse(new Date())/100000),
                        success : function (d) {
                            var data_ctlist = d.result.data;
                            var ctlistLen = data_ctlist.length;
                            var ctlistStr = '';
                            // 如果仅有一个结果,且这个结果与输入一致, 直接添加即可
                            if(ctlistLen == 1 && (data_ctlist[0].chinese_name == $slider_add_input.val() || data_ctlist[0].name == $slider_add_input.val())){
                                _hideCitiesLayer();
                                _postCityCode(data_ctlist[0].loc_code);
                            }
                            // 没有匹配的结果
                            else if(ctlistLen == 0){
                                if($('.slider_add_none').length){
                                    $('.slider_add_btns').hide(0);
                                    $('.slider_add_none').fadeIn(300);
                                    setTimeout(function () {
                                        if($('.slider_add_none').length && $('.slider_add_btns').length){
                                            $('.slider_add_btns').show(0);
                                            $('.slider_add_none').hide(0);
                                        }
                                    }, 2000);
                                }
                            }
                            else{
                                for(var i = 0; i < ctlistLen; i ++){
                                    ctlistStr += ('\
                                        <span class="slider_add_ctlist_i" data-citycode="' + data_ctlist[i].loc_code + '">' + (data_ctlist[i].chinese_name == '' ? data_ctlist[i].name : data_ctlist[i].chinese_name)  + '</span>\
                                    ');
                                    $('.slider_add_ctlist').html(ctlistStr);
                                }
                                _setCitiesLayerStates(1, true);
                                // _renderCitiesList(data_ctlist);
                            }
                            citiesAddRQ = false;
                        }
                    });
                });
                // 点击每个城市条目执行搜索
                $citiesLayer.on('click', '.slider_add_ctlist_i', function () {
                    var curCtCode = $(this).data('citycode');
                    _hideCitiesLayer();
                    _postCityCode(curCtCode);
                });
                _showCitiesLayer();
            }
            else if($citiesLayer.is(':hidden')){
                _showCitiesLayer();
            }
        });

        function _showCitiesLayer () {
            $.popUp.open({
                trigger: 'click',
                maskClickClose: false,
                autoSize: false,
                width: 290,
                height: 180,
                popEle: $citiesLayer
            });
        }

        function _hideCitiesLayer () {
            $.popUp.close();
            _setCitiesLayerStates(0, true);
        }
        function _citiesLayerLoad () {
            // body...
        }
        // 浮层切换到第几个级别
        function _setCitiesLayerStates (stateCode, isClear) {
            var ctLayerHth;
            // 第一级菜单
            if(stateCode == 0){
                isClear && $('.slider_add_ctlist').html('');
                $('.slider_add_tt1').add('.slider_add_ctlist')
                    .hide(0);
                $.popUp.resize({
                    width : 290,
                    height : 180,
                    callBack : function () {
                        $('.slider_add_tt0').add('.slider_add_input').add('.slider_add_btns')
                            .stop().fadeIn(300);
                    }
                });
                $('.slider_add_c').animate({
                    'width': '290px',
                    'height': '180px'
                }, 400);
            }
            // 第二级菜单
            else if(stateCode == 1){
                $('.slider_add_tt0').add('.slider_add_input').add('.slider_add_btns').add('.slider_add_none')
                    .hide(0);
                isClear && $('.slider_add_input').val('');
                ctLayerHth = Math.ceil($('.slider_add_ctlist').find('.slider_add_ctlist_i').length / 2) * 32 + 80;
                $.popUp.resize({
                    width : 290,
                    height : ctLayerHth,
                    callBack : function () {
                        $('.slider_add_tt1').add('.slider_add_ctlist')
                            .stop().fadeIn(300);
                    }
                });
                $('.slider_add_c').animate({
                    'width' : '290px',
                    'height' : ctLayerHth + 'px'
                }, 400);
            }
        }

        // 判断是否支持某属性
        function _attrSupportCk (attr, label) {
            var label = label || 'div';
            var node = document.createElement(label);
            return attr in node;
        }
        function _clipString (str, a, b) {
            var s = str.replace(/([^\x00-\xff])/g, "\x00$1");
            return (s.length < b) ? str : (s.substring(a, b - 3).replace(/\x00/g, '') + '...');
        }

        function _postCityCode (citycode) {
            var submitUrl = 'http://weather.sina.com.cn/api/open/setUserLocation/' + citycode +'/';
            jsonp(submitUrl, function(data){
                if( 0 == data.result.status.code ){
                    $dc.addClass("hasSet");
                    var durl = 'http://weather.sina.com.cn/' + data.result.data.loc_url;
                    var dname = data.result.data.loc_name;
                    $dcName.attr("href",durl);
                    $dcName.html(dname);
                    setNameWidth();
                }
            }, "submitDefaultCity");
        }

        // 修改默认
        var $sdcb_change = $("#sdcb_change");
        $sdcb_change.on("click",function(){
            $.publish('addDefaultCity');
            return false;
        });
    })();
