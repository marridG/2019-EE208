<script type="text/javascript">
$(function(){
/* 热图 */
var allpage=$("#slide-show > li").length;
var ipage=1;
$("#prev").click(function(){
	if(!$("#slide-show").is(":animated")){
			if(ipage==1){
				$("#slide-show").animate({left:'-='+960*(allpage-1)},"slow");
				ipage=allpage;
			}else{
				$("#slide-show").animate({left:'+='+960},"slow");
				ipage--;
				}
			}
});
$("#next").click(function(){
	if(!$("#slide-show").is(":animated")){
			if(ipage==allpage){
				$("#slide-show").animate({left:'0px'},"slow");
				ipage=1;
			}else{
				$("#slide-show").animate({left:'-='+960},"slow");
				ipage++;
			}
			
		}
});
/* 四个热点栏目 */
$(".lmlist li").hover(function(){
	var $this_id=$(this).attr("data-id");
	$("#sec2 #"+$this_id).show().siblings().hide();
	$(this).find("img.on").show();
	$(this).find("img.off").hide();
	$(this).siblings().find("img.on").hide();
	$(this).siblings().find("img.off").show();
});
/* 军情 */
$(".jqcnt > .left,.topright").mouseover(function(){
	$(this).find("p").show();
}).mouseout(function(){
	$(this).find("p").hide();
});
});
</script>
