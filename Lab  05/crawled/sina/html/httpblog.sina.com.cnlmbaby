					Login.prototype.showLogin = function (userInfo) {
						this.getBlogNick(userInfo,function(data){
							window.msgBus.publish('login');
							this.state.uid = data.uid;
							this.state.nick = data.nick;
							this.changeALFCookie('on', data.uid);
							var reg = /##uid##/g;
							$('.logined').html(this.tpl.replace(reg, this.state.uid)
											   .replace('**userNick**', this.state.nick)
											   .replace('**msgNum**', '-')
											   .replace('**pointNum**', '-')
											   .replace('**attNum**', '-')).show();
							this.getNums();
							//显示 登陆用户状态
							$('.not-login').hide();
						});
					};

					Login.prototype.getBlogNick = function(userInfo,cb){
						var _self = this;
						$.ajax({
							url: 'http://hs.blog.sina.com.cn/user/nick/' + userInfo.uid,
							dataType:'jsonp',
							success:function(data){
								if(data.code == 0 && data.data){
									cb.call(_self,{
										uid:userInfo.uid,
										nick:data.data[userInfo.uid]
									});
								}else{
									cb.call(_self,userInfo);
								}
							},
							error: function(){
								cb.call(_self,userInfo);
							}
						});
					}

					Login.prototype.getNums = function () {
						//https://comets.blog.sina.com.cn/notice?uid=1595082642&varname=STK_1526372762938105
						var variableUnread = 'var_unread_' + (new Date().getTime());
						var variableAtt = 'requestId_' + (new Date().getTime());
						var _self = this;
						window[variableUnread] = null;
						window[variableAtt] = null;
						//未读消息
						$.ajax({
							url: 'https://comets.blog.sina.com.cn/notice?uid=' + this.state
							.uid + '&varname=' + variableUnread,
							dataType: 'script',
							success: function () {
								if (window[variableUnread].code && window[
									variableUnread].code === 'A00006') {
									var numInfo = window[variableUnread].data[_self.state
																			  .uid];
									$('#unreadnotices').text(_self.limitNum(numInfo.unreadnotices || 0));
								}
							},
							error: function () {

							}
						});
						//关注&用户积分
						$.ajax({
							url: 'http://blogtj.sinajs.cn/api/get_attention_num.php?uid=' +
							this.state.uid + '&suid=' + this.state.uid +
							'&attention=suid&userpointuid=' + this.state.uid +
							'&s=1&varname=' + variableAtt,
							dataType: 'script',
							success: function () {
								if (window[variableAtt] && window[variableAtt].code === 'A00006') {
									$('#user-point').text(_self.limitNum(window[variableAtt].data.userpoint));
									$('#fans').text(_self.limitNum(window[variableAtt].data.num[_self.state.uid]));
								}
							}
						})
					}

					Login.prototype.checkLoginStatus = function (cb) {
						var _self = this,
							isReq = false;
						if (!isReq) {
							//状态发生了改变了
							//状态改变，查询subp以及接口方式获取用户情况。

							if (window.sinaSSOController && window.sinaSSOController.getSinaCookie(true)) {
								var subpInfo = window.sinaSSOController.getSinaCookie(true);
								cb({
									nick: subpInfo.nick,
									uid: subpInfo.uid
								});
							} else {
								isReq = true;
								//control接口
								$.ajax({
									url: 'http://control.blog.sina.com.cn/riaapi/check_login.php',
									dataType: 'jsonp',
									success: function (data) {
										if (data.uid) {
											cb({
												nick: data.nick,
												uid: data.uid
											});
										} else {
											cb();
										}
									},
									error: function () {
										cb();
									},
									complete: function () {
										isReq = false;
									}
								})
							}

						}
					}

					Login.prototype.isLogin = function () {
						return this.state.isLogin;
					}
					Login.prototype.changeLoginState = function () {
						this.state.isLogin = !this.state.isLogin;
					}
					var blogLogin = new Login();

					window.blogLogin = blogLogin;
				});

			});
		});
	})();
