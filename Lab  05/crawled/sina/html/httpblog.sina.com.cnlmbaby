                                        this.state.uid = userInfo.uid;
                                        this.state.nick = userInfo.nick;
                                        $('.logined').html(this.tpl.replace(/##uid##/g, this.state.uid)
                                            .replace('**userNick**', this.state.nick)
                                            .replace('**msgNum**', '-')
                                            .replace('**pointNum**', '-')
                                            .replace('**attNum**', '-')).show();
                                        this.getNums();
                                        //TODO:显示 登陆用户状态
                                        $('.not-login').hide();
