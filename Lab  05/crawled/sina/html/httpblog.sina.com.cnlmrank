				}

				arr_sub_bang[i].blog_pubdate = arr_sub_bang[i].blog_pubdate.substr(5,5);

				//
				var html_cell_num	 = '<div class="num"><span>' + (i+1) + '</span></div>';
				var html_cell_title	 = '<div class="link335bbd textL"><a target="_blank" href="'+ arr_sub_bang[i].blog_url +'?tj=1" title="'+arr_sub_bang[i].blog_title+'">'+html_cell_title_s+'</a></div>';
				var html_cell_asc_nums = arr_sub_bang[i].asc_nums.replace(/(?=(\d{3})+$)\B/g, ",");
				var html_cell_nick	 = '<div class="link-ul"><a href="http://blog.sina.com.cn/u/'+arr_sub_bang[i].uid+'" target="_blank" class="a02" title="'+arr_sub_bang[i].uname+'">'+html_cell_nick_s+'</a></div>';
				var html_cell_date	 = arr_sub_bang[i].blog_pubdate;
				var html_cell_article_hits = arr_sub_bang[i].article_hits.replace(/(?=(\d{3})+$)\B/g, ",");
