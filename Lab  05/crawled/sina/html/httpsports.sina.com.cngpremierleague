					</div>
					<script type="text/javascript">
					$(function(){
						// 积分榜初始化
						function initJF(limit){
							
							$.ajax({
								url: "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=football&_sport_s_=opta&_sport_a_=teamOrder&type=4",
								type: "GET",
								dataType: "jsonp",
								data:{
									dpc: 1
								},
								cache:true,
								jsonpCallback:'CB_F6BA8554_ABF1_47A8_BF51_E803E9D3C669',
								success: function(data){
									var dataList = data.result.data, $w = $(".jf_rank .rk_list ul");
									setJFInfo(dataList, 0, limit, $w);
								},
								error: function(){
									window.console && window.console.log("error");
								}
							})
						}
						function setJFInfo(d, i, limit, $w){
							var temp = d[i];
							if(!d[i]||i>=limit) return;
							if(i==0){
								
								$.ajax({
									url: "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=football&_sport_s_=opta&_sport_a_=getteam&id=" + temp.sl_id + "&dpc=1",
									type: "GET",
									dataType: "jsonp",
									data:{
										dpc: 1
									},
									cache:true,
									jsonpCallback:'CB_F6BA8554_ABF1_47A8_BF51_E803E9D3C663_'+ temp.sl_id,
									success: function(data){
										$w.append('<li class="hvr"><span class="rk_order">'+temp.team_order+'</span><span class="jf_team"><a href="http://match.sports.sina.com.cn/football/team.php?id='+temp.sl_id+'&l_type='+ temp.l_type +'" target="_blank"><img src="'+data.result.data.logo_small+'" alt="'+temp.team_cn+'" />'+temp.team_cn+'</a></span><span class="jf_count">'+temp.count+'</span><span class="rk_result">'+temp.score+'</span></li>');
										i++;
										setJFInfo(d, i, limit, $w);
									},
									error:function(){
										window.console && window.console.log("error");
									}
								})
							} else{
								$w.append('<li><span class="rk_order">'+temp.team_order+'</span><span class="jf_team"><a href="http://match.sports.sina.com.cn/football/team.php?id='+temp.sl_id+'&l_type='+ temp.l_type +'" target="_blank">'+temp.team_cn+'</a></span><span class="jf_count">'+temp.count+'</span><span class="rk_result">'+temp.score+'</span></li>');
								i++;
								setJFInfo(d, i, limit, $w);
							}
						}
						// 积分榜初始化，参数表示可展示条数，可配置
						initJF(7);
						// 射手榜初始化
						function initShs(season, limit){
							
							$.ajax({
								url: "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=football&_sport_s_=opta&_sport_a_=playerorder&item=13&type=4&season=" + season + "&limit=" + limit + "&dpc=1",
								type: "GET",
								dataType: "jsonp",
								data:{
									dpc: 1
								},
								cache:true,
								jsonpCallback:'CB_F6BA8554_ABF1_47A8_BF51_E803E9D3C661_'+[season, limit].join('_').replace(/-/g,'_'),
								success: function(data){
									var dataList = data.result.data, len = dataList.length;
									for(var i=0; i<len; i++){
										var singleData = dataList[i];
										$(".scorer_rank .rk_list ul").append('<li><span class="rk_order">'+singleData.num+'</span><span class="sc_team"><a href="http://match.sports.sina.com.cn/football/player.php?id='+singleData.player_id+'" target="_blank">'+singleData.player_name+'</a> | <a href="http://match.sports.sina.com.cn/football/team.php?id='+singleData.sl_team_id+'&l_type='+ singleData.l_type +'" target="_blank">'+singleData.team_name+'</a></span><span class="rk_result">'+singleData.item1+'</span></li>');
									}
								},
								error: function(){
									window.console && window.console.log("error");
								}
							})
						}
						// 射手榜初始化，参数表示season和limit，可配置
						initShs(2019, 7);
						// 榜单切换
						$(".rk_tabs span").click(function(){
							var $this = $(this), index = $this.index();
							$this.addClass("rk_tabs_on").siblings().removeClass("rk_tabs_on");
							$(".rk_conts .rk_cont").eq(index).show().siblings().hide();
						})
						// 滑过效果
						$(".rk_list").delegate("li:not('.rk_list_title')","mouseenter",function(){
							var $this = $(this), $img = $this.find(".jf_team img");
							$this.addClass("hvr").siblings().removeClass("hvr");							
						})
					})
					</script>
				
