		<div class="contest">
						<ul id="contest_list" class="contest_list clearfix"></ul>
			<script type="text/javascript">
			;(function(){
				// 赛程参数设置，LeagueType可能的值为4（英超）,7（足总杯）,8（联赛杯）
				var leagueType = "4" || 4,
					date = "2019-10-08" || "2014-09-17",
					span = "15" || 10;
				var args = {LeagueType: leagueType, begin: date, timespan: span};
				var $contestList = $("#contest_list");
				
				$.ajax({
					url: "//api.sports.sina.com.cn/?p=sports&s=sport_client&a=index&_sport_t_=livecast&_sport_a_=dateMatches&LeagueType=" + args.LeagueType + "&begin=" + args.begin + "&timespan=" + args.timespan,
					type: "GET",
					dataType: "jsonp",
					data:{
						dpc: 1
					},
					cache:true,
					jsonpCallback:'CB_F6BA8554_ABF1_47A8_BF51_E803E9D3C665_'+[args.LeagueType, args.begin, args.timespan].join('_').replace(/-/g,'_'),
					success: function(r){
						var dataList = r.result.data;
						for(key in dataList){
							if(key>=10) break;
							var tempData = dataList[key];
							$contestList.append("<li></li>");
							var $contestLi = $contestList.find("li").eq(key);
							$contestLi.append('<p class="cst_time">'+tempData.date.replace(/^\d+-/,'')+' '+tempData.time+'</p>')
							var status = tempData.match_status, cls = "", mtLink = "", linkHtml = "";
							if(status == "3"){ //完赛
								cls ="over";
								mtLink = tempData.NewsUrl;
								tempData.VideoUrl&&(linkHtml+='<a href="'+tempData.VideoUrl+'" target="_blank" class="mrg_r_5">视频集锦</a>');
								tempData.ImgUrl&&(linkHtml+='<a href="'+tempData.ImgUrl+'" target="_blank">图集</a>');
							} else if(status == "2"){ //比赛中
								cls ="on";
								mtLink = tempData.LiveUrl;
								
								videoliveurl = tempData.VideoLiveUrl;
								
								if(videoliveurl.indexOf('smgbb.cn') != -1){
									var datetime=new Date();
									var s=datetime.getFullYear()+""+(datetime.getMonth()+1)+""+datetime.getDate()+""+datetime.getHours()+""+datetime.getMinutes()+""+datetime.getSeconds();
									videoliveurl = videoliveurl + '?source=sina&subsource=premierleagueIndex&tokenId='+s;
								}
								
								videoliveurl&&(linkHtml+='<a href="'+videoliveurl+'" target="_blank" class="mrg_r_5">视频直播</a>');
								tempData.LiveUrl&&(linkHtml+='<a href="'+tempData.LiveUrl+'" target="_blank">图文直播</a>');
							} else if(status == "1"){  //未开赛
								cls ="will";
								videoliveurl = tempData.VideoLiveUrl;
								
								if(videoliveurl.indexOf('smgbb.cn') != -1){
									var datetime=new Date();
									var s=datetime.getFullYear()+""+(datetime.getMonth()+1)+""+datetime.getDate()+""+datetime.getHours()+""+datetime.getMinutes()+""+datetime.getSeconds();
									videoliveurl = videoliveurl + '?source=sina&subsource=premierleagueIndex&tokenId='+s;
								}
								mtLink = tempData.LiveUrl;
								videoliveurl&&(linkHtml+='<a href="'+videoliveurl+'" target="_blank" class="mrg_r_5">视频直播</a>');
								tempData.LiveUrl&&(linkHtml+='<a href="'+tempData.LiveUrl+'" target="_blank">图文直播</a>');
							}
							$contestLi.append('<a class="cst_team" href="'+mtLink+'" target="_blank"><p><span>'+tempData.Team1+'</span><em>'+(status != "1"?tempData.Score1:"")+'</em></p><p><span>'+tempData.Team2+'</span><em>'+(status != "1"?tempData.Score2:"")+'</em></p></a>');
							$contestLi.append('<p class="cst_link">'+linkHtml+'</p>')
							$contestLi.addClass("cst_"+cls);
						}
						$contestList.find("li:last").addClass("cst_last");
					},
					error: function(){

					}
				})
			})()
			</script>
		</div>
		<!-- 赛程 end -->
