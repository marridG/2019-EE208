	<script type="text/javascript">
	(function($){
		var $wrap = $("#main_wrapper"),
		    random = Math.floor(Math.random()*3),
		    scrollW = window.screen.width;
		if(scrollW <= 1280){
			$wrap.addClass("main_wrapper_0");
		} else{
			$wrap.addClass("main_wrapper_" + random);
		}
	})(jQuery);
