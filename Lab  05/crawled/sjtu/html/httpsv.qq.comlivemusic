        window_width = document.documentElement.clientWidth;
        window_height = document.documentElement.clientHeight;

        if( window_width < 1680 && window_width > 1240){
            $('body').addClass("page_wide_width").removeClass("page_normal_width")
        }else if( window_width < 1240 ){
            $('body').addClass("page_normal_width").removeClass("page_wide_width")
        }else if(window_width > 1680){
